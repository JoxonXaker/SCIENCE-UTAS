{   
    "builds": [
        {
            "src": "project/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.11.4" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "project/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"         
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"         
        }
        
    ]
}